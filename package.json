{
	"name": "telegram-api-fetch",
	"version": "0.2.0",
	"description": "TypeScript type definitions and client for the Telegram Bot API using Zod schemas",
	"homepage": "https://github.com/Dogalyir/telegram-api-fetch#readme",
	"bugs": {
		"url": "https://github.com/Dogalyir/telegram-api-fetch/issues"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dogalyir/telegram-api-fetch.git"
	},
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"release": "bumpp --commit --push --tag",
		"test": "bun test",
		"test:coverage": "bun test --coverage",
		"test:watch": "bun test --watch",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.5",
		"@types/bun": "^1.3.2",
		"bumpp": "^10.3.1",
		"bunup": "^0.15.14",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"dependencies": {
		"zod": "^4.1.12"
	}
}
